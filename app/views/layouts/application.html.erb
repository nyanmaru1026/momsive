<!DOCTYPE html>
<html>
  <head>
    <title>Momsive</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class = "header">
      <div class = "header-upper">
        <%= link_to "Momsive", "#"%>
        <%= link_to "投稿する", "#"%>
      </div>
    </div>
    <div class = "contents">
      <%= yield %>
    </div>
  </body>
</html>
